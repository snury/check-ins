module.name_mapper='^ducks\(\/?.*\)$' -> '<PROJECT_ROOT>/src/ducks/\1'
module.name_mapper='^utils\(\/?.*\)$' -> '<PROJECT_ROOT>/src/utils/\1'
module.name_mapper='^store\(\/?.*\)$' -> '<PROJECT_ROOT>/src/store/\1'
module.name_mapper='^kit\(\/?.*\)$' -> '<PROJECT_ROOT>/src/kit/\1'
module.name_mapper='^middleware\(\/?.*\)$' -> '<PROJECT_ROOT>/src/middleware/\1'
module.name_mapper='^config\(\/?.*\)$' -> '<PROJECT_ROOT>/src/config/\1'

