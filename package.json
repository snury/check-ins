{
  "name": "check-ins",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint:style": "sass-lint -c .sass-lint.yml -v -q -f",
    "lint:js": "esw src",
    "lint:js:fix": "esw --fix src",
    "lint:js:watch": "esw -w src",
    "flow": "flow check",
    "flow:watch": "flow-watch",
    "test": "NODE_PATH=src CI=true react-scripts test --env=jsdom",
    "test:watch": "NODE_PATH=src react-scripts test --env=jsdom",
    "precommit": "yarn run flow && yarn run test && yarn run lint:style && yarn run lint:js",
    "dev": "NODE_PATH=src react-scripts start & node ./server/app.js",
    "prod": "NODE_PATH=src react-scripts build"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.3",
    "@material-ui/icons": "^3.0.1",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-mung": "^0.5.1",
    "flexboxgrid": "^6.3.1",
    "geoip-lite": "^1.3.2",
    "helmet": "^3.13.0",
    "history": "^4.7.2",
    "ipinfo": "^1.3.8",
    "lodash": "^4.17.10",
    "mongoose": "^5.2.14",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-api-middleware": "^2.3.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "custom-react-scripts": "^0.2.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "flow-bin": "^0.80.0",
    "flow-watch": "^1.1.1",
    "jest-localstorage-mock": "^2.1.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.3.0",
    "sass-lint": "^1.12.1"
  }
}
